  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "ncp src/templates dist/src/templates && ncp src/i18n dist/src/i18n",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "typeorm": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
    "migration:create": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/DB",
    "migration:generate": "cross-var npm run build && npm run typeorm -- migration:generate ./src/migrations/DB",
    "migration:run": "cross-var npm run build && npm run typeorm -- migration:run",
    "migration:show": "npm run typeorm -- migration:show",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
